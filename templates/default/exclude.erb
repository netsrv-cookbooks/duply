#
# Managed by Chef - local changes may be overwritten
#

<% @includes.each do |glob| %>
  <% unless glob.empty? %>
+ <%= @prefix %><%= @prefix.empty? ? glob : glob.gsub(/^\//, "") %>
  <% end %>
<% end %>
<% @excludes.each do |glob| %>
  <% unless glob.empty? %>
- <%= @prefix %><%= @prefix.empty? ? glob : glob.gsub(/^\//, "")  %>
  <% end %>
<% end %>
